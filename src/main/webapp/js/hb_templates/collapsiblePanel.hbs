<div class="panel panel-default collapsible-panel">
	<div class="panel-heading">
		{{panelHeading}}
		<button class="collapse-btn" data-toggle="collapse" data-target="#{{panelBodyId}}" type="button">
			<i class="expand-icon fa fa-plus"></i>
			<i class="collapse-icon fa fa-minus"></i>
		</button>
	</div>
	<div class="panel-body collapse {{#if opened}}in{{/if}}" id="{{panelBodyId}}">
	</div>
</div>